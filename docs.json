{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "urlDNA - Documentation",
  "colors": {
    "primary": "#1DC775",
    "light": "#1DC775",
    "dark": "#1DC775"
  },
  "background":{
    "color": {
      "dark": "#303030",
      "light": "#E3E3E3"
    }
  },
  "fonts": {
    "family": "Montserrat",
    "source": "https://fonts.gstatic.com/s/montserrat/v29/JTUSjIg1_i6t8kCHKm459Wlhyw.woff2",
    "format": "woff2",
    "weight": 400
  },
  "appearance":{
    "default": "dark",
    "strict": true
  },
  "favicon": "/images/logo_green.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Documentation",
            "pages": [
              "docs/introduction",
              "docs/scan",
              "docs/search",
              "docs/brand",
              "docs/query",
              "docs/takedown",
              "docs/chat",
              "docs/integration"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/python",
              "api-reference/quota-limit"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/create-scan",
              "api-reference/endpoint/get-scan",
              "api-reference/endpoint/search",
              "api-reference/endpoint/fast-check",
              "api-reference/endpoint/scan-feedback",
              "api-reference/endpoint/user-agent",
              "api-reference/endpoint/viewport"
            ]
          }
        ]
      },
      {
        "tab": "Use Cases",
        "groups": [
          {
            "group": "Use Cases",
            "pages": [
              "use-cases/introduction"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "use-cases/cyber-threat-intelligence",
              "use-cases/security-operation-center-integration",
              "use-cases/brand-monitoring"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/logo_green.png",
    "dark": "/images/logo_green.png"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Go to ulrDNA",
      "href": "https://urldna.io"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/urldna",
      "github": "https://github.com/urldna",
      "linkedin": "https://linkedin.com/company/urldna"
    }
  }
}